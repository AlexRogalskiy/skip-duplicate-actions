{
  "name": "skip-duplicate-action-runs",
  "version": "1.0.0",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build": "ncc build src/index.ts"
  },
  "dependencies": {
    "@actions/core": "^1.2.5",
    "@actions/github": "^4.0.0",
    "@vercel/ncc": "^0.24.1"
  },
  "devDependencies": {
    "typescript": "^4.0.3",
    "husky": "^4.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run build && git diff --exit-code dist/"
    }
  }
}
